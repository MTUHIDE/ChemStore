@page
@model ChemStoreWebApp.Pages.Debug.LocationModel
@using ChemStoreWebApp.Utilities

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Container</title>
</head>
<body>
<table class="table">
    <thead>
        <tr>
            <th>
                LocationID
            </th>
            <th>
                ParentID
            </th>
            <th>
                Level
            </th>
            <th>
                Name
            </th>
            <th>
                DepartmentID
            </th>
            <th>
                SupervisorID
            </th>
            <th>
                IsHidden
            </th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.X_Location) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.LocationID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ParentID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Level)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DepartmentID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SupervisorID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IsHidden)
            </td>
        </tr>
}
    </tbody>
</table>
</body>
</html>
