@page
@model ChemStoreWebApp.Pages.Admin.Chemicals.DetailsModel

@{
    ViewData["Title"] = "Details";
    Layout = "~/Pages/Shared/_Layout.cshtml";
}

<h1>Details</h1>

<div>
    <h4>Chemical</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Chemical.ChemicalName)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Chemical.ChemicalName)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Chemical.Quantity)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Chemical.Quantity)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Chemical.Manufacturer)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Chemical.Manufacturer)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Chemical.CatalogNumber)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Chemical.CatalogNumber)
        </dd>
    </dl>
</div>
<div>
    <a asp-page="./Edit" asp-route-id="@Model.Chemical.CasNumber">Edit</a> |
    <a asp-page="./Index">Back to List</a>
</div>
